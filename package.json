{
  "name": "@starnote/publish-github",
  "version": "0.3.0",
  "description": "辰记发布Github SDK",
  "scripts": {
    "build": "rm -rf lib && tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/star-note/publish-github.git"
  },
  "author": {
    "name": "辰记ludejun",
    "email": "ludejun@live.cn",
    "url": "https://github.com/star-note/publish-github"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/star-note/publish-github/issues"
  },
  "keywords": [],
  "homepage": "https://github.com/star-note/publish-github",
  "devDependencies": {
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "js-base64": "^3.7.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "sideEffects": false
}
