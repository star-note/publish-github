{
  "name": "@starnote/publish-github",
  "version": "0.5.3",
  "description": "辰记发布Github SDK",
  "scripts": {
    "tsc": "rm -rf lib && tsc",
    "build": "webpack --config ./webpack.config.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/star-note/publish-github.git"
  },
  "author": "辰记ludejun <ludejun@live.cn> (https://github.com/star-note/publish-github)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/star-note/publish-github/issues"
  },
  "keywords": [],
  "homepage": "https://github.com/star-note/publish-github",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.82.0"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "js-base64": "^3.7.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "private": false,
  "sideEffects": false,
  "main": "index.ts",
  "directories": {
    "lib": "dist"
  }
}
